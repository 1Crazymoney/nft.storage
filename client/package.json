{
  "name": "nft.storage",
  "version": "0.0.0-dev",
  "description": "nft.storage client",
  "license": "(Apache-2.0 AND MIT)",
  "type": "module",
  "types": "./dist/src/lib.d.ts",
  "module": "./src/lib.js",
  "exports": {
    ".": {
      "import": "./src/lib.js",
      "browser": "./src/lib.js"
    },
    "./src/api.js": {
      "default": "./src/api.js"
    }
  },
  "typesVersions": {
    "*": {
      "src/*": [
        "dist/src/*"
      ]
    }
  },
  "scripts": {
    "test": "node test/run.js",
    "build": "tsc --build",
    "typedoc": "typedoc --entryPoints src  --out ../docs/client",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@ssttevee/multipart-parser": "0.1.8",
    "@web-std/blob": "2.0.1",
    "@web-std/fetch": "1.0.0",
    "@web-std/file": "1.0.1",
    "@web-std/form-data": "2.0.0"
  },
  "devDependencies": {
    "husky": "4.2.5",
    "ipfs-unixfs-importer": "6.0.1",
    "ipld": "0.29.0",
    "ipld-dag-pb": "0.22.0",
    "ipld-in-memory": "8.0.0",
    "lint-staged": "10.2.11",
    "multicodec": "3.0.1",
    "multiformats": "4.5.3",
    "multihashing-async": "2.1.2",
    "playwright-test": "2.1.0",
    "prettier": "2.2.1",
    "typedoc": "0.20.32",
    "typescript": "4.2.3",
    "web-streams-polyfill": "3.0.2",
    "zora": "4.0.2"
  },
  "lint-staged": {
    "*.js": [
      "prettier --no-semi --write",
      "git add"
    ]
  },
  "homepage": "https://github.com/ipfs-shipyard/nft.storage/tree/main/client",
  "bugs": "https://github.com/ipfs-shipyard/nft.storage/issues"
}
